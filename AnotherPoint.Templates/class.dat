@model Type

@{
    Layout = null;
}

namespace @Model.Namespace
{
@{WriteLiteral(Model.IsPublic ? "public" : "private");}
@{WriteLiteral(Model.IsSealed ? "sealed" : "");}
@{WriteLiteral(Model.IsAbstract ? "abstract" : "");} 
    class @Model.Name
    {
        @foreach (var field in @Model.GetFields())
        {
            WriteLiteral(field.Name);
        }
    }
}