@using AnotherPoint.Common
@model AnotherPoint.Entities.Property

@{
	string setMethodAccessModificator = Model.SetMethod.AccessModifyer.AsString();
	string getMethodAccessModificator = Model.GetMethod.AccessModifyer.AsString();

	if (getMethodAccessModificator == setMethodAccessModificator)
	{
		setMethodAccessModificator = "";
	}

	string typeName = Model.Type.IsGeneric.HasValue && Model.Type.IsGeneric.Value ?
						Model.Type.FullName + "<" + string.Join(",", Model.Type.GenericTypes) + ">" :
						Model.Type.FullName;
}

@getMethodAccessModificator @typeName @Model.Name { get; @setMethodAccessModificator set; }