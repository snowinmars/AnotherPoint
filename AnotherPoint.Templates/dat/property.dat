@model AnotherPoint.Entities.Property

@{
	setMethodAccessModificator = Model.SetMethod.AccessModifyer.AsString();
	getMethodAccessModificator = Model.GetMethod.AccessModifyer.AsString();

	if (getMethodAccessModificator == setMethodAccessModificator)
	{
		setMethodAccessModificator = "";
	}
}

@getMethodAccessModificator @propertyType @Model.Name { get; @setMethodAccessModificator set; }